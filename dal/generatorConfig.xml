<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!--数据库驱动jar -->
	<classPathEntry
		location="D:/alibaba/m2-repo/mysql/mysql-connector-java/5.1.36/mysql-connector-java-5.1.36.jar" />

	<!--数据库连接 -->
	<context id="hr" targetRuntime="MyBatis3">
		<property name="suppressAllComments" value="true" />
		<property name="useActualColumnNames" value="false" />
		<plugin type="com.alibaba.mybatis.generator.plugins.MysqlPagingPlugin" />
		<jdbcConnection driverClass="org.gjt.mm.mysql.Driver"
			connectionURL="jdbc:mysql://182.92.105.100:3306/qyp" userId="root"
			password="qyp1234">
		</jdbcConnection>

		<!--默认false Java type resolver will always use java.math.BigDecimal if 
			the database column is of type DECIMAL or NUMERIC. -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建 使用Maven生成在target目录下，会自动创建） -->
		<javaModelGenerator targetPackage="com.qyp.dal.pojo"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!--生成SQLMAP文件 -->
		<sqlMapGenerator targetPackage="com.qyp.dal.mapper"
			targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!--生成Dao文件 可以配置 type="XMLMAPPER"生成xml的dao实现 context id="DB2Tables" 修改targetRuntime="MyBatis3" -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.qyp.dal.mapper" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!--对应数据库表 mysql可以加入主键自增 字段命名 忽略某字段等 -->
		<!-- <table schema="sc_mop" tableName="msg_plan" -->
		<!-- domainObjectName="MsgPlan" enableCountByExample="true" -->
		<!-- enableUpdateByExample="true" enableDeleteByExample="true" -->
		<!-- enableSelectByExample="true" selectByExampleQueryId="true"> -->
		<!-- <generatedKey column="ID" sqlStatement="mysql" identity="true" /> -->
		<!-- </table> -->

		<table schema="qrp" tableName="activity" domainObjectName="Activity"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="true">
			<generatedKey column="ID" sqlStatement="mysql" identity="true" />
		</table>

		<table schema="qrp" tableName="member" domainObjectName="Member"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="true">
			<generatedKey column="ID" sqlStatement="mysql" identity="true" />
		</table>

		<table schema="qrp" tableName="member_activity" domainObjectName="MemberActivity"
			enableCountByExample="true" enableUpdateByExample="true"
			enableDeleteByExample="true" enableSelectByExample="true"
			selectByExampleQueryId="true">
			<generatedKey column="ID" sqlStatement="mysql" identity="true" />
		</table>

	</context>
</generatorConfiguration>
